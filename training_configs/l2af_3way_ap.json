{
    "dataset_reader": {
        "type": "l2af_simple_v2_binary",
        "lazy": true,
        "num_context_answers": 3,
        "token_indexers": {
            "token_characters": {
                "type": "characters",
                "character_tokenizer": {
                    "byte_encoding": "utf-8",
                    "end_tokens": [
                        260
                    ],
                    "start_tokens": [
                        259
                    ]
                },
                "min_padding_length": 5
            },
            "tokens": {
                "type": "single_id"
            }
        }
    },
    "iterator": {
        "type": "bucket",
        "batch_size": 32,
        "biggest_batch_first": true,
        "max_instances_in_memory": 1000,
        "sorting_keys": [
            [
                "passage",
                "num_tokens"
            ],
            [
                "all_qa",
                "num_tokens"
            ]
        ]
    },
    "model": {
        "type": "l2af_3way_ap_modular_binary",
        "cartesian_attn": {
            "identity": true
        },
        "choicelevelenc": {
            "type": "lstm",
            "bidirectional": true,
            "hidden_size": 150,
            "input_size": 400,
            "num_layers": 1
        },
        "cqaattnmat": {
            "identity": true,
            "projector": {
                "activations": "linear",
                "dropout": 0.3,
                "hidden_dims": 300,
                "input_dim": 300,
                "num_layers": 1
            }
        },
        "dropout": 0.3,
        "gate_qdep_penc": {
            "gate": {
                "activations": "linear",
                "dropout": 0.3,
                "hidden_dims": 600,
                "input_dim": 600,
                "num_layers": 1
            }
        },
        "initializer": [
            [
                ".*linear_layers.*weight",
                {
                    "type": "xavier_normal"
                }
            ],
            [
                ".*token_embedder_tokens._projection.*weight",
                {
                    "type": "xavier_normal"
                }
            ]
        ],
        "is_mfa_enc": true,
        "is_qac_ap": true,
        "is_qdep_penc": true,
        "mfa_enc": {
            "attn_pooling": "max",
            "gate": {
                "activations": "linear",
                "dropout": 0.3,
                "hidden_dims": 600,
                "input_dim": 600,
                "num_layers": 1
            },
            "num_factor": 4,
            "projector": {
                "activations": "tanh",
                "dropout": 0.3,
                "hidden_dims": 1200,
                "input_dim": 300,
                "num_layers": 1
            }
        },
        "mfa_rnn": {
            "type": "lstm",
            "bidirectional": true,
            "hidden_size": 150,
            "input_size": 600,
            "num_layers": 1
        },
        "pcattnmat": {
            "identity": true,
            "projector": {
                "activations": "linear",
                "dropout": 0.3,
                "hidden_dims": 300,
                "input_dim": 300,
                "num_layers": 1
            }
        },
        "pqaattnmat": {
            "identity": true,
            "projector": {
                "activations": "linear",
                "dropout": 0.3,
                "hidden_dims": 300,
                "input_dim": 300,
                "num_layers": 1
            }
        },
        "pseqlevelenc": {
            "type": "lstm",
            "bidirectional": true,
            "hidden_size": 150,
            "input_size": 400,
            "num_layers": 1
        },
        "qaseqlevelenc": {
            "type": "lstm",
            "bidirectional": true,
            "hidden_size": 150,
            "input_size": 400,
            "num_layers": 1
        },
        "qdep_penc_rnn": {
            "type": "lstm",
            "bidirectional": true,
            "dropout": 0.3,
            "hidden_size": 150,
            "input_size": 600,
            "num_layers": 1
        },
        "shared_rnn": true,
        "text_field_embedder": {
            "token_characters": {
                "type": "character_encoding",
                "dropout": 0.3,
                "embedding": {
                    "embedding_dim": 20,
                    "num_embeddings": 262
                },
                "encoder": {
                    "type": "cnn",
                    "embedding_dim": 20,
                    "ngram_filter_sizes": [
                        5
                    ],
                    "num_filters": 100
                }
            },
            "tokens": {
                "type": "embedding",
                "embedding_dim": 300,
                "pretrained_file": "data/embeddings/glove.840B.300d.txt.gz",
                "trainable": false
            }
        },
        "with_knowledge": true
    },
    "train_data_path": "data/dataset/train.json",
    "validation_data_path": "data/dataset/dev.json",
    "test_data_path": "data/dataset/test_i.json",
    "trainer": {
        "cuda_device": 2,
        "learning_rate_scheduler": {
            "type": "reduce_on_plateau",
            "factor": 0.5,
            "mode": "max",
            "patience": 3
        },
        "num_epochs": 25,
        "num_serialized_models_to_keep": 2,
        "optimizer": {
            "type": "adam",
            "lr": 0.001
        },
        "patience": 5,
        "validation_metric": "+auc"
    },
    "evaluate_on_test": true
}
